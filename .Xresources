! special
URxvt.letterSpace: 0
URxvt.font:	    xft:Roboto Mono for Powerline:size=11

! special
*.foreground:   #c5c8c6
*.background:   #1d1f21
*.cursorColor:  #c5c8c6

! black
*.color0:       #282a2e
*.color8:       #373b41

! red
*.color1:       #cc0000
*.color9:       #ff4444

! green
*.color2:       #669900
*.color10:      #99cc00

! yellow
*.color3:       #ff8800
*.color11:      #ffbb33

! blue
*.color4:       #0099cc
*.color12:      #33b5e5

! magenta
*.color5:       #9933cc
*.color13:      #aa66cc

! cyan
*.color6:       #27ae60
*.color14:      #2ecc71

! white
*.color7:       #7f8c8d
*.color15:      #95a5a6

!
! scrollbar
!
! enable scrollbar
URxvt*scrollBar:          false
! scrollbar width
URxvt*thickness:          5
! scrollbar color
URxvt*scrollColor:        #1E1E1E
! scrollbar trough area color
!URxvt*troughColor:        #333333
! scrollbar without a trough
!URxvt*scrollBar_floating: false
! scrollbar to the right
URxvt*scrollBar_right:    true
! scrollbar style
URxvt*scrollstyle:        plain

! URxvt by default does not support using the control+arrows to advance or
! retreat one word at a time. The following two codes re-enable this.
URxvt.keysym.C-Right    : \033[1;5C
URxvt.keysym.C-Left     : \033[1;5D
